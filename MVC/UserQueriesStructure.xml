	<metadata>
		<userquery name="Новый запрос">
			<expression>Select o.OrderID,
  c.CustomerID,
  s.ShipperID,
  o.ShipCity
From Orders o
  Inner Join Customers c On o.CustomerID = c.CustomerID
  Inner Join Shippers s On s.ShipperID = o.OrderID
Where o.ShipCity = 'A'</expression>
		</userquery>
	</metadata>
